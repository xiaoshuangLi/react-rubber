{
  "name": "page-map",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:xiaoshuangLi/page-map.git"
  },
  "scripts": {
    "start": "npm run production",
    "dev": "npm-run-all development:server:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:server:build development:services",
    "development:server:build:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "development:server:build": "webpack --mode development --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development:server": "nodemon ./server/index.entry.js --watch ./server --watch ./public/server",
    "development:services": "npm-run-all --parallel development:server",
    "production": "npm-run-all production:client:build production:server:build production:services",
    "production:client:build": "webpack --mode production --colors --display-error-details --config ./webpack/webpack.config.client.production.babel.js",
    "production:server:build": "webpack --mode production --colors --display-error-details --config ./webpack/webpack.config.server.production.babel.js",
    "production:server": "better-npm-run production:server",
    "production:services": "npm-run-all --parallel production:server"
  },
  "betterScripts": {
    "production:server": {
      "command": "node ./server/index.entry.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "express",
    "react",
    "redux"
  ],
  "author": "Xiaoshuang",
  "homepage": "http://xiaodifang.club",
  "dependencies": {
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-transition-group": "^2.2.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "redux-undo": "^0.6.1",
    "require-dir": "^0.3.0",
    "socket.io": "^2.0.4"
  },
